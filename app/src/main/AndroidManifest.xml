<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.elliott.nfc2" >

    <uses-permission android:name="android.permission.NFC" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-feature
        android:name="android.hardware.nfc"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.elliott.nfc2.AddActivity"> </activity>
        <activity android:name="com.example.elliott.nfc2.AdminActivity"></activity>
        <activity android:name="com.example.elliott.nfc2.DetailActivity"></activity>
        <activity android:name="com.example.elliott.nfc2.UserListActivity"></activity>
    </application>

</manifest>
